<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

    <type-mapping>
        <sql-type jdbc-type="DECIMAL" hibernate-type="Long"
                  precision="8" >
        </sql-type>
    </type-mapping>

    <table-filter match-name=".*" match-schema="MEMO" exclude="false" />

</hibernate-reverse-engineering>